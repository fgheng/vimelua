      "end, {silent = true, noremap = true})"],
    "description": "vim.keymap.set()"
  },
  "autocmd": {
      "prefix": "autocmd",
       "body": [
            "vim.api.nvim_create_autocmd({\"${1}\"}, {",
            "   pattern = {\"${2}\"}",
            "   callback = function(args)",
            "       ${3}",
            "   end,",
            "})"
        ],
        "description": "nvim_create_autocmd"
  }
